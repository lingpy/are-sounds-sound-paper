#Nexus
Begin MrBayes;
    execute ../../data/correspondences_nexus/constenlachibchan.nex;
    prset brlenspr = clock:uniform;
    lset rates=gamma;
    prset brlenspr = clock:uniform;
    prset clockvarpr=igr;
    lset coding=all;
    mcmcp stoprule=yes burninfrac=0.25 stopval=0.01 filename=output/constenlachibchan samplefreq=1000 printfreq=1000;
    mcmc ngen=20000000 nchains=2 nruns=2;
    sumt;
    sump;
    q;
end;
