#Nexus
Begin MrBayes;
    execute ../../data/combined/walworthpolynesian.nex;
    prset brlenspr = clock:uniform;
    prset clockvarpr = igr;
    lset rates=gamma;
    mcmcp stoprule=yes burninfrac=0.25 stopval=0.01 filename=output/walworthpolynesian_combined samplefreq=1000 printfreq=1000 append=no;
    mcmc ngen=1000000000 nchains=4 nruns=2;
    sumt;
    sump;
    q;
end;
