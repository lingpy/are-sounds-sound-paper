#Nexus
Begin MrBayes;
    execute ../../data/cognate_classes_nexus/dravlex.nex;
    prset brlenspr = clock:uniform;
    lset rates=gamma;
    prset brlenspr = clock:uniform;
    prset clockvarpr=igr;
    lset coding=all;
    mcmcp stoprule=yes burninfrac=0.25 stopval=0.01 filename=output/dravlex_cc samplefreq=1000 printfreq=1000;
    mcmc ngen=20000000 nchains=2 nruns=2;
    sumt;
    sump;
    q;
end;
